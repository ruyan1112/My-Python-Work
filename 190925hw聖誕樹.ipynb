     "text": [
      "Populating the interactive namespace from numpy and matplotlib\n"
     ]
    }
   ],
   "source": [
    "%pylab inline\n",
    "import numpy as np\n",
    "\n",
    "def xmastree(n):\n",
    "    print(\" \"*n+\"✩\")\n",
    "    for i in range(1,n+1):\n",
    "        print( \" \"*(n-i) + \"*\"*i+ \"||\" + \"*\"*i)"
   ]
  },
